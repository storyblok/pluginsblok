{
  "name": "field-plugin",
  "version": "0.0.0",
  "description": "SDK for creating Field Plugins for Storyblok.",
  "author": {
    "name": "Storyblok",
    "email": "it@storyblok.com",
    "url": "https://www.storyblok.com/"
  },
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@10.15.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "postinstall": "pnpm build",
    "build": "nx run-many -t=build --projects=@storyblok/field-plugin,@storyblok/manifest-helper,@storyblok/lib-helpers,@storyblok/field-plugin-cli,sandbox",
    "build:lib": "nx run @storyblok/field-plugin:build",
    "build:cli": "nx run @storyblok/field-plugin-cli:build",
    "build:sandbox": "nx run sandbox:build",
    "test": "pnpm test:lib",
    "test:lib": "nx run @storyblok/field-plugin:test",
    "test:lib:watch": "nx run @storyblok/field-plugin:test:watch",
    "test:cli": "nx run @storyblok/field-plugin-cli:test",
    "test:cli:watch": "nx run @storyblok/field-plugin-cli:test:watch",
    "test:types": "nx run-many -t=test:types --projects=packages/field-plugin/**",
    "lint": "eslint .",
    "dev:demo": "nx run demo:dev",
    "dev:cli": "nx run @storyblok/field-plugin-cli:dev",
    "dev:lib": "nx run @storyblok/field-plugin:dev",
    "dev:sandbox": "nx run sandbox:dev"
  },
  "devDependencies": {
    "@eslint/js": "9.23.0",
    "@storyblok/mui": "0.3.1",
    "@types/node": "^22.17.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.19",
    "eslint-plugin-vue": "9.33.0",
    "globals": "15.15.0",
    "kleur": "4.1.5",
    "prettier": "^3.5.3",
    "prompts": "2.4.2",
    "semver": "7.7.1",
    "tsx": "3.14.0",
    "typescript": "5.8.2",
    "typescript-eslint": "8.29.0",
    "vite": "5.4.16",
    "vite-plugin-dts": "1.7.3",
    "vue-eslint-parser": "9.4.3",
    "zx": "7.2.3"
  }
}
